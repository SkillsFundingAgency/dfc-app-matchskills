@model DFC.App.MatchSkills.WebUI.ViewModels.SearchOccupationResultsViewModel

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, DFC.Personalisation.CommonUI

@foreach (var occupation in Model.Occupations)
{
    <govukRadioButton Id=@occupation.Id value="yes">
        <govukRadioLabel AdditionalClass="radio-container-item" Text=@occupation.Name For=@occupation.Name></govukRadioLabel>
    </govukRadioButton>

}
